{# This template will configure the CIN to DPIC interfaces#}
#File created {{cur_date}} by {{email}}
{# Configure interfaces on Primary CIN Device#}
##--Primary CIN Device--##
{% for row in csv_data %}
interface {{row.interface}}  
description {{cbr8name}} {{row.description}}
cdp
mtu 9200
ipv4 address {{row.prefix | next_nth_usable(1)}}  {{row.prefix | ipaddr('netmask')}}
load-interval 30
!
{% endfor -%}
